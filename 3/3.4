#include<iostream> 
#include <vector> 
using namespace std; 
int main() { 
    int given, del, c = 0; 
    vector <int> mas; 
    cout << "Input num ";  
    cin >> given; 
    cout << "Input num to delete ";  
    cin >> del; 
    for(int i = 0; given != 0; i++) { 
        mas.push_back (given % 10); //get the last character 
        given /= 10; //get rid of the 0 
        c++; //length of the num 
    }  
    for(int i = c-1; i >= 0; i--) { 
        if (mas[i] != del) { 
        cout << mas[i];  
        } 
    } 
    return 0; 
}
